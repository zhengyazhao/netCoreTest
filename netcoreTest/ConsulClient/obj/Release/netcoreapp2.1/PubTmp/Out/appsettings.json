{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "ServiceDiscovery": {
    "ServiceName": "UserApi",
    "Version": "1.0.0",
    "HealthCheckTemplate": "/api/health",
    "Endpoints": [ "http://localhost:8001" ],
    "Consul": {
      "HttpEndpoint": "http://127.0.0.1:8500",
      "DnsEndpoint": {
        "Address": "127.0.0.1",
        "Port": 8600
      }
    }
  },
  "AllowedHosts": "*"
}
